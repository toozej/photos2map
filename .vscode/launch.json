{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run photos2map",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["--dir", "${workspaceFolder}/in/", "--output", "gpx"]
        },
        {
            "name": "Get photos2map version",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["version"]
        }
    ]
}